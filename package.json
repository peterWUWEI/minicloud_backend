{
   "name": "minicloud_backend",
   "version": "1.0.0",
   "description": "Backend service for Minicloud Japan",
   "main": "index.js",
   "scripts": {
      "start": "ts-node src/index.ts",
      "build": "rm -rf dist && tsc -p .",
      "postinstall": "tsc",
      "watch-node": "nodemon dist/index.js",
      "watch-ts": "tsc -w",
      "dev": "ts-node-dev src/index.ts",
      "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/peterWUWEI/minicloud_backend.git"
   },
   "author": "Peter Wu Wei",
   "license": "ISC",
   "bugs": {
      "url": "https://github.com/peterWUWEI/minicloud_backend/issues"
   },
   "homepage": "https://github.com/peterWUWEI/minicloud_backend#readme",
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.0.0",
      "express": "^4.17.2",
      "mysql": "^2.14.1",
      "reflect-metadata": "^0.1.13",
      "ts-node-dev": "^1.1.8",
      "typeorm": "0.2.44",
      "typescript": "^4.5.5"
   },
   "devDependencies": {
      "@types/node": "^16.11.10",
      "ts-node": "10.4.0",
      "typescript": "4.5.2"
   }
}
